<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<window>
	<defaultcontrol>9000</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<animation effect="fade" time="250">WindowOpen</animation>
	<animation effect="fade" time="250">WindowClose</animation>
	<controls>
		<control type="group">
			<posx>0</posx>
			<posy>0</posy>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<texture border="40">background-panel.png</texture>
			</control>
			<!-- ** Required ** Do not change <id> or <type> -->

			<control type="image">
				<description>page header tab icon</description>
				<posx>20</posx>
				<posy>20</posy>
				<width>234</width>
				<height>80</height>
				<texture>enigma_logo.png</texture>

			</control>
			
			<control type="button" id="5555">
				<description>Search image</description>
				<ondown>8000</ondown>
				<posx>1100</posx>
				<posy>30</posy>
				<width>50</width>
				<height>50</height>
				<visible>true</visible>
				<fadetime>200</fadetime>
				<texturefocus>search_icon_glow.png</texturefocus>
				<texturenofocus>search_icon.png</texturenofocus>
				<aspectratio>keep</aspectratio>
			</control>
						
			<control type="button" id="9000">
				<onright>9001</onright>
				<onup>5555</onup>
				<left>200</left>
				<top>200</top>
				<width>100</width>
				<height>100</height>
				<texturefocus>primewire.png</texturefocus>
				<texturenofocus>primewire.png</texturenofocus>
				<animation effect="zoom" end="120" center="360,360" time="200">Focus</animation>
				<font>font10_title</font>
				<textcolor>white</textcolor>
				<align>center</align>
				<aligny>top</aligny>
				<textoffsety>115</textoffsety>
				<!--<onclick></onclick>-->
				<label>Primewire Indexer</label>
			</control>
			
			<control type="button" id="9001">
				<onright>9001</onright>
				<onleft>9000</onleft>
				<onup>5555</onup>
				<left>310</left>
				<top>200</top>
				<width>100</width>
				<height>100</height>
				<texturefocus>icefilms.png</texturefocus>
				<texturenofocus>icefilms.png</texturenofocus>
				<animation effect="zoom" end="120" center="360,360" time="200">Focus</animation>
				<font>font10_title</font>
				<textcolor>white</textcolor>
				<align>center</align>
				<aligny>top</aligny>
				<textoffsety>115</textoffsety>
				<!--<onclick></onclick>-->
				<label>Icefilms Indexer</label>
			</control>
			
		</control>
	</controls>
</window>
